  <version>0.0.0</version>
  <description>Message and action definitions for MoveIt state server interaction.</description>

  <maintainer email="aljoscha97@web.de">Aljoscha Schmidt</maintainer>
  <author email="aljoscha97@web.de">Aljoscha Schmidt</author>
  <license>MIT</license> <!-- Replace TODO with your actual license -->

  <buildtool_depend>ament_cmake</buildtool_depend>

  <build_depend>rosidl_default_generators</build_depend>
